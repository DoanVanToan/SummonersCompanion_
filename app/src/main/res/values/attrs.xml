<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RobotoTextView">
        <attr name="typeface" />
    </declare-styleable>

    <attr name="typeface" format="enum">
        <enum name="roboto_light" value="0" />
        <enum name="roboto_regular" value="1" />
        <enum name="roboto_bold" value="2" />
        <enum name="arista_light" value="3" />
    </attr>

    <declare-styleable name="ProgressView">
        <attr name="progress_value" format="integer" />
        <attr name="background_color" format="enum">
            <enum name="blue" value="0" />
            <enum name="green" value="1" />
            <enum name="red" value="2" />
            <enum name="violet" value="3" />
        </attr>
    </declare-styleable>


</resources>